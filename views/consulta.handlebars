<div class="container text-center">
    <div class="row">
        <!-- Navbar and other elements remain unchanged -->
    </div>
    <div class="row">
        <div class="col-1">
            &nbsp;
        </div>
        <div class="col bg-white">
            <p class="text-start fs-5">Cadastrar - Agendamento de Potenciais Clientes</p>
            <p class="text-start fs-6">Sistema Utilizado para agendamento de serviços.</p>
            <div class="container text-center">
                <div class="row bg-primary">
                    <div class="col text-light">
                        Nome
                    </div>
                    <div class="col text-light">
                        Telefone
                    </div>
                    <div class="col text-light">
                        Origem
                    </div>
                    <div class="col text-light">
                        Data de Contato
                    </div>
                    <div class="col text-light">
                        Observação
                    </div>
                    <div class="col text-light">
                        Ações
                    </div>
                </div>
                <div class="row">
                    <div class="col text-light">
                        &nbsp;
                    </div>
                    <div class="col text-light">
                        &nbsp;
                    </div>
                    <div class="col text-light">
                        &nbsp;
                    </div>
                    <div class="col text-light">
                        &nbsp;
                    </div>
                    <div class="col text-light">
                        &nbsp;
                    </div>
                    <div class="col text-light">
                        &nbsp;
                    </div>
                </div>
                {{#each data}} <!-- Corrected variable name here -->
                    <div class="row">
                        <div class="col">
                            {{nome}}
                        </div>
                        <div class="col">
                            {{telefone}}
                        </div>
                        <div class="col">
                            {{origem}}
                        </div>
                        <div class="col">
                            {{data_contato}}
                        </div>
                        <div class="col">
                            {{observacao}}
                        </div>
                        <div class="col">
                            <button type="button" class="btn btn-primary"><a href="/editar/{{id}}" style="color:inherit">Editar</a></button>
                            <button type="button" class="btn btn-primary"><a href="/excluir/{{id}}" style="color:inherit">Excluir</a></button>
                        </div>
                    </div>
                {{/each}}
            </div>
        </div>
        <div class="col-1">
            &nbsp;
        </div>
    </div>
</div>
